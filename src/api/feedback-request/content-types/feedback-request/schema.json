{
  "kind": "collectionType",
  "collectionName": "feedback_requests",
  "info": {
    "singularName": "feedback-request",
    "pluralName": "feedback-requests",
    "displayName": "Заявки формы обратной связи",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "minLength": 12,
      "maxLength": 18
    },
    "email": {
      "type": "string",
      "required": true,
      "regex": "^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$"
    },
    "description": {
      "type": "text"
    }
  }
}
